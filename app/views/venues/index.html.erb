<h1>Listing venues</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Street address</th>
    <th>City</th>
    <th>State</th>
    <th>Region</th>
    <th>Postal code</th>
    <th>Instructions</th>
    <th>Hours</th>
    <th>Grand opening</th>
    <th>Grand closing</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @venues.each do |venue| %>
  <tr>
    <td><%= venue.name %></td>
    <td><%= venue.description %></td>
    <td><%= venue.street_address %></td>
    <td><%= venue.city %></td>
    <td><%= venue.state %></td>
    <td><%= venue.region %></td>
    <td><%= venue.postal_code %></td>
    <td><%= venue.instructions %></td>
    <td><%= venue.hours %></td>
    <td><%= venue.grand_opening %></td>
    <td><%= venue.grand_closing %></td>
    <td><%= link_to 'Show', venue %></td>
    <td><%= link_to 'Edit', edit_venue_path(venue) %></td>
    <td><%= link_to 'Destroy', venue, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Venue', new_venue_path %>
