<h1>Listing feeds</h1>

<table>
  <tr>
    <th>Url</th>
    <th>Feed url</th>
    <th>Etag</th>
    <th>Last modified</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feeds.each do |feed| %>
  <tr>
    <td><%= feed.url %></td>
    <td><%= feed.feed_url %></td>
    <td><%= feed.etag %></td>
    <td><%= feed.last_modified %></td>
    <td><%= link_to 'Show', feed %></td>
    <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
    <td><%= link_to 'Destroy', feed, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Feed', new_feed_path %>
